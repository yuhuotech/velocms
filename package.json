{
  "name": "velocms",
  "version": "0.1.0",
  "private": true,
  "description": "A flexible, multi-theme blog/CMS system for content creators",
  "scripts": {
    "dev": "bash scripts/start-dev.sh",
    "dev:simple": "next dev -p 3002",
    "build": "next build",
    "start": "next start -p 3002",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate --config=drizzle.config.mjs",
    "db:migrate": "drizzle-kit migrate --config=drizzle.config.mjs",
    "db:push": "drizzle-kit push --config=drizzle.config.mjs",
    "db:studio": "drizzle-kit studio --config=drizzle.config.mjs",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.11.1",
    "@tiptap/extension-image": "^3.15.3",
    "@tiptap/extension-link": "^3.15.3",
    "@tiptap/extension-placeholder": "^3.15.3",
    "@tiptap/extension-typography": "^3.15.3",
    "@tiptap/pm": "^3.15.3",
    "@tiptap/react": "^3.15.3",
    "@tiptap/starter-kit": "^3.15.3",
    "@types/bcryptjs": "^2.4.6",
    "@vercel/blob": "^2.0.0",
    "@vercel/kv": "^2.0.0",
    "@vercel/postgres": "^0.9.0",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^10.0.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "drizzle-orm": "^0.31.0",
    "highlight.js": "^11.11.1",
    "kysely": "^0.28.9",
    "lucide-react": "^0.378.0",
    "mysql2": "^3.16.0",
    "nanoid": "^5.0.0",
    "next": "^15.0.0",
    "next-auth": "^5.0.0-beta.20",
    "next-themes": "^0.4.0",
    "postgres": "^3.4.8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^9.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "shiki": "^1.6.0",
    "tailwind-merge": "^2.3.0",
    "tiptap-markdown": "^0.9.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^15.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.12.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.0",
    "drizzle-kit": "^0.22.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.0",
    "postcss": "^8.4.0",
    "prettier": "^3.2.0",
    "prettier-plugin-tailwindcss": "^0.6.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.4.0",
    "vitest": "^1.6.0"
  }
}
